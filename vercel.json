{
  "version": 2,
  "builds": [
    {
      "src": "telegram_bot.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9",
        "pythonVersion": "3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}